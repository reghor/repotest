		timeToIdleSeconds="120" timeToLiveSeconds="120" overflowToDisk="true" />

	<cache name="getAllItems" maxElementsInMemory="100" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="0" timeToLiveSeconds="1200" />

</ehcache>
